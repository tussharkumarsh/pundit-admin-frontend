    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon server.js",
    "build:prod": "ng build --prod",
    "ssr:dev": "ng build --dev && ng build --dev --app 1 && webpack --progress --colors",
    "build:universal": "ng build --prod && ng build --prod && webpack --progress"
